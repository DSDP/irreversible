<h1>Escuelas</h1>
<input class="input-file" type="file" id="file">

<a {{action 'process'}} class="scan-app-process">Procesar</a>
{{#if model.length}}
  <table class="table-minimal">
    <thead>
      <tr>
        <th>
          Nombre
        </th>
        <th colspan="3">
          Actiones
        </th>
      </tr>
    </thead>
    <tbody>
      {{#each school in model}}
        <tr>
          <td>
            {{school.name}}
          </td>  
          <td>
            {{link-to "Editar" "data-entry.festa.schools.edit" school}}
          </td>
        </tr>
      {{/each}}
      {{infinity-loader infinityModel=model}}
    </tbody>
  </table>
{{else}}
  <p id="blankslate">
    No aval found
  </p>
{{/if}}